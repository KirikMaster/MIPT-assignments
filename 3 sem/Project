import cv2
import numpy as np

cap = cv2.VideoCapture(0);
print(cap.get(cv2.CAP_PROP_FRAME_WIDTH))
cap.set(3,1280) # Установление длины окна
cap.set(4,700)  # Ширина окна
print(cap.get(3))
print(cap.get(4))

while(True):
    ret, frame = cap.read()
    cv2.imshow("frame",frame)
    cv2.waitKey(0)
cap.release()
cv2.destroyAllWindows()
